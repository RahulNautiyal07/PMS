<%- include('header') %>


    <div class="container">
        <div class="row">
  <h1> Password Category Lists</h1>
  <br>
  <div class="table-responsive">
   <table class="table table-bordered ">
       <thead>
           <tr>
               <td>Password Category Name</td>
               
               <td>Action</td>
               
           </tr>
       </thead>
       <tbody>
           <% if(records.length > 0){ 
            records.forEach(function(row){

         
           %>

           <tr>
              <td><%= row.password_category %></td>
             
              <!-- <td></td> -->
             <td> <a class="btn btn-primary btn-xs" href="/passCategory/edit/<%= row._id %>"><i class="fa fa-edit"></i></a>
             <a class="btn btn-danger btn-xs" href="/passCategory/delete/<%= row._id %>"><i class="fa fa-trash"></i></a>   </td>

           </tr>
       <% })} %>
       </tbody>

   </table>
   </div>
 </div>
</div>   
  </body>
</html>
